{
  "expo": {
    "name": "Bhozzan",
    "slug": "bhozzan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./App/assets/App-Logos.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./App/assets/App-Logos.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package":"apkpure.Bhozzan",
      "adaptiveIcon": {
        "foregroundImage": "./App/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      // "permissions": [
      //   "ACCESS_COARSE_LOCATION",
      //   "ACCESS_FINE_LOCATION",
      //   "ACCESS_BACKGROUND_LOCATION"
      // ]
    },
    "web": {
      "favicon": "./App/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Bhozzan to use your location."
        }
      ]
    ]
  }
}
